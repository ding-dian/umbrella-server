<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.volunteer.mapper.SignUpRecordMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.volunteer.entity.SignUpRecord">
        <id column="id" property="id" />
        <result column="volunteer_id" property="volunteerId" />
        <result column="volunteer_activity_id" property="volunteerActivityId" />
        <result column="deleted" property="deleted" />
        <result column="create_at" property="createAt" />
    </resultMap>

    <!-- 通用查询结果列 -->

    <select id="selectByVolunteerActivityID" resultType="com.volunteer.entity.SignUpRecord">
        select * from sign_up_record where volunteer_activity_id=#{volunteerActivityId}
    </select>

</mapper>
